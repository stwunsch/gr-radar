<?xml version="1.0"?>
<block>
  <name>R v RCS Simulator</name>
  <key>radar_r_v_rcs_simulator_cc</key>
  <category>RADAR</category>
  <import>import radar</import>
  <make>radar.r_v_rcs_simulator_cc($rnge, $velocity, $rcs, $samp_rate, $center_freq, $amplitude)</make>
  <param>
    <name>Range</name>
    <key>rnge</key>
    <type>float_vector</type>
  </param>
  <param>
    <name>Velocity</name>
    <key>velocity</key>
    <type>float_vector</type>
  </param>
  <param>
    <name>RCS</name>
    <key>rcs</key>
    <type>float_vector</type>
  </param>
  <param>
    <name>Sample rate</name>
    <key>samp_rate</key>
    <type>int</type>
  </param>
  <param>
    <name>Center frequency</name>
    <key>center_freq</key>
    <type>float</type>
  </param>
  <param>
    <name>Amplitude input signal</name>
    <key>amplitude</key>
    <type>int</type>
  </param>
  <sink>
    <name>IQ in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>IQ out</name>
    <type>complex</type>
  </source>
</block>
