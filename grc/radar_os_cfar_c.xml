<block>
  <name>OS-CFAR</name>
  <key>radar_os_cfar_c</key>
  <category>RADAR</category>
  <import>import radar</import>
  <make>radar.os_cfar_c($samp_rate, $samp_compare, $samp_protect, $rel_threshold, $mult_threshold, $len_key, $msg_out)</make>
  <param>
    <name>Sample rate</name>
    <key>samp_rate</key>
    <type>int</type>
  </param>
  <param>
    <name>Compared samples</name>
    <key>samp_compare</key>
    <type>int</type>
  </param>
  <param>
    <name>Protected samples</name>
    <key>samp_protect</key>
    <type>int</type>
  </param>
  <param>
    <name>Relative threshold</name>
    <key>rel_threshold</key>
    <type>float</type>
  </param>
  <param>
    <name>Multiplier threshold</name>
    <key>mult_threshold</key>
    <type>float</type>
  </param>
  <param>
    <name>Packet length key</name>
    <key>len_key</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <param>
    <name>Message port key</name>
    <key>msg_out</key>
    <value>"cfar_out"</value>
    <type>string</type>
  </param>
  <sink>
    <name>IQ in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>Msg out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
