<block>
  <name>Doppler RCS Simulator</name>
  <key>radar_doppler_rcs_simulator_cc</key>
  <category>RADAR</category>
  <import>import radar</import>
  <make>radar.doppler_rcs_simulator_cc($range, $velocity, $rcs, $samp_rate, $center_freq, $amplitude)</make>
  <param>
    <name>Range</name>
    <key>range</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Velocity</name>
    <key>velocity</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>RCS</name>
    <key>rcs</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Sample rate</name>
    <key>samp_rate</key>
    <type>int</type>
  </param>
  <param>
    <name>Center frequency</name>
    <key>center_freq</key>
    <type>float</type>
  </param>
  <param>
    <name>Incoming signal amplitude</name>
    <key>amplitude</key>
    <type>float</type>
  </param>
  <sink>
    <name>IQ in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>IQ out</name>
    <type>complex</type>
  </source>
</block>
